datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Source {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  earnings String[]
  name     String
  workers  String[]
  
}

  model Transactions {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  workers      String[]
  createdAt    String
  status       String
  total        String
  percentage   String
  amount       String
}

  model Subscription {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  amount       String
  reason       String
  type         String
  date         String
  status       String
}

  model Earning {
  id         String @id @default(auto()) @map("_id") @db.ObjectId
  amount     Float
  createdAt  String
  lead       String
  modelId    String @db.ObjectId
  percentage String
  status     String
  total      String
  workerId   String @db.ObjectId
}

model Model {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  age         String
  country     String
  date        String
  description String
  drive       String
  email       String
  image       String
  name        String   @unique
  password    String
  telegram    String
  workers     String[]
  earnings    String[]
}

model Worker {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  earnings String[]
  modelId  String
  name     String
  profit   String
}
